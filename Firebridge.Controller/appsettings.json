{
  "DiscoveryServerPort": 47000,
  "DiscoveryServerSecret": "SECRETaa",
  "DiscoveryDelay": 1000,
  "AgentServerPort": 47001,
  "Serilog": {
    "Using": [ "Serilog.Sinks.Console", "Serilog.Sinks.File" ],
    "MinimumLevel": "Verbose",
    "WriteTo": [
      {
        "Name": "Console",
        "Args": {
          "theme": "Serilog.Sinks.SystemConsole.Themes.AnsiConsoleTheme::Literate, Serilog.Sinks.Console",
          "outputTemplate": "[{Timestamp:HH:mm:ss} {Level:u3}] <{ThreadName}> <{ThreadId}> {Message:lj} <s:{SourceContext}>{NewLine}{Exception}"
        }
      },
      {
        "Name": "File",
        "Args": {
          "path": "Logs/log.txt",
          "outputTemplate": "[{Timestamp:HH:mm:ss} {Level:u3}] <{ThreadName}> <{ThreadId}> {Message:lj} <s:{SourceContext}>{NewLine}{Exception}"
        }
      }
    ],
    "Enrich": [ "WithThreadId" ]
  }
}